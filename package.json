{
  "name": "vrchat-world-manager",
  "version": "1.0.0",
  "main": "dist-electron/main.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/yamato3010/vrchat-world-manager.git"
  },
  "directories": {
    "doc": "doc"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "generate-template-db": "cross-env DATABASE_URL=\"file:./template.db\" prisma db push --schema ./prisma/schema.prisma --accept-data-loss",
    "prepackage": "npm run generate-template-db"
  },
  "author": "Yamato3010",
  "license": "ISC",
  "description": "A world manager tool for VRChat",
  "devDependencies": {
    "@electron-forge/cli": "^7.10.2",
    "@electron-forge/maker-deb": "^7.10.2",
    "@electron-forge/maker-rpm": "^7.10.2",
    "@electron-forge/maker-wix": "^7.10.2",
    "@electron-forge/maker-zip": "^7.10.2",
    "@electron-forge/plugin-auto-unpack-natives": "^7.10.2",
    "@electron-forge/plugin-fuses": "^7.10.2",
    "@electron/fuses": "^1.8.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@tailwindcss/vite": "^4.1.17",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/electron": "^1.4.38",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^4.0.15",
    "@vitest/ui": "^4.0.15",
    "autoprefixer": "^10.4.22",
    "cross-env": "^10.1.0",
    "electron": "^39.2.3",
    "jsdom": "^27.2.0",
    "postcss": "^8.5.6",
    "prisma": "5.22.0",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6",
    "vitest": "^4.0.15"
  },
  "dependencies": {
    "@prisma/client": "5.22.0",
    "axios": "^1.13.2",
    "electron-updater": "^6.7.3",
    "png-metadata": "^1.0.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.6"
  }
}